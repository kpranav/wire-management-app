# Root .env.example - Copy this to .env and update with your values

# Docker Hub
DOCKERHUB_USERNAME=your-dockerhub-username

# EC2
EC2_HOST=your-ec2-public-ip

# QA Environment
QA_DB_PASSWORD=secure-qa-password
QA_JWT_SECRET=secure-qa-jwt-secret

# UAT Environment
UAT_DB_PASSWORD=secure-uat-password
UAT_JWT_SECRET=secure-uat-jwt-secret

# Production Environment
PROD_DB_PASSWORD=secure-prod-password
PROD_JWT_SECRET=secure-prod-jwt-secret

# Generate secure secrets with:
# openssl rand -base64 32
